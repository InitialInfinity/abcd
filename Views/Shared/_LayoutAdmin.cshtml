<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - InternationFamilyWeb</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />

    <link rel="stylesheet" href="~/InternationFamilyWeb.styles.css" asp-append-version="true" />
    <!-- Favicons -->
    <link href="assets/img/favicon.png" rel="icon">
    <link href="assets/img/apple-touch-icon.png" rel="apple-touch-icon">

    <!-- Google Fonts -->
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Open+Sans:ital,wght@0,300;0,400;0,500;0,600;0,700;1,300;1,400;1,600;1,700&family=Montserrat:ital,wght@0,300;0,400;0,500;0,600;0,700;1,300;1,400;1,500;1,600;1,700&family=Raleway:ital,wght@0,300;0,400;0,500;0,600;0,700;1,300;1,400;1,500;1,600;1,700&display=swap" rel="stylesheet">
    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.10.0/css/all.min.css" rel="stylesheet">
    <!-- Vendor CSS Files -->
    <link href="~/assets/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">
    <link href="~/assets/vendor/bootstrap-icons/bootstrap-icons.css" rel="stylesheet">
    <link href="~/assets/vendor/aos/aos.css" rel="stylesheet">
    <link href="~/assets/vendor/glightbox/css/glightbox.min.css" rel="stylesheet">
    <link href="~/assets/vendor/swiper/swiper-bundle.min.css" rel="stylesheet">
    <link href="~/assets/vendor/remixicon/remixicon.css" rel="stylesheet">

    <!-- Template Main CSS File -->
    <link href="~/assets/css/main.css" rel="stylesheet">

    <style>

        .carousel-item::before {
            content: "";
            position: absolute;
            top: 0;
            left: 0;
            width: 100%;
            height: 100%;
            background: rgba(27, 47, 69, 0.6); /* Adjust the color and opacity as needed */
            z-index: 1;
        }

        .carousel-item {
            position: relative;
        }

            .carousel-item img {
                position: relative;
                z-index: 0;
            }

        <style >
        .carousel-item {
            position: relative;
        }

        .carousel-caption {
            position: absolute;
            top: 50%;
            left: 50%;
            transform: translate(-50%, -50%);
            text-align: center;
            color: red; /* Dark white color */
        }

            .carousel-caption h3 {
                color: red; /* Dark white color */
            }

        .ftco_navbar {
            position: fixed;
            width: 100%;
            z-index: 1000;
            transition: top 0.3s;
        }

            .ftco_navbar.scrolled {
                top: 0;
            }

        .social-icons a {
            color: #333;
            margin: 0 10px;
            font-size: 20px;
        }

            .social-icons a:hover {
                color: #ff7f50;
            }

        h1.display-4 {
            color: #ff7f50;
        }

        .lead {
            color: #666;
        }

        .card-container-wrapper {
            position: relative;
        }

        .card-container {
            display: flex;
            overflow-x: auto;
            scroll-snap-type: x mandatory;
            scroll-behavior: smooth;
            padding: 20px 0;
        }

            .card-container::-webkit-scrollbar {
                display: none;
            }

        .card {
            flex: 0 0 30%;
            margin: 10px;
            scroll-snap-align: start;
        }

            .card img {
                height: 200px;
                object-fit: cover;
            }

        .nav-button {
            position: absolute;
            top: 50%;
            transform: translateY(-50%);
            background-color: rgba(0, 0, 0, 0.5);
            border: none;
            color: white;
            padding: 10px;
            cursor: pointer;
            z-index: 1;
        }

            .nav-button.left {
                left: 0;
            }

            .nav-button.right {
                right: 0;
            }

        .team-item:hover .team-social {
            opacity: 1;
        }

        .team-img .team-social {
            position: absolute;
            width: 100%;
            height: 100%;
            top: 0;
            left: 0;
            display: flex;
            align-items: center;
            justify-content: center;
            background: rgba(68, 66, 90, 0.8);
            transition: .5s;
            z-index: 1;
            opacity: 0;
        }

        .ftco_navbar {
            position: fixed;
            width: 100%;
            z-index: 1000;
            transition: top 0.3s;
        }

            .ftco_navbar.scrolled {
                top: 0;
            }

        .social-icons a {
            color: #333;
            margin: 0 10px;
            font-size: 20px;
        }

            .social-icons a:hover {
                color: #ff7f50;
            }

        h1.display-4 {
            color: #ff7f50;
        }

        .lead {
            color: #666;
        }

        .card-container-wrapper {
            position: relative;
        }

        .card-container {
            display: flex;
            overflow-x: auto;
            scroll-snap-type: x mandatory;
            scroll-behavior: smooth;
            padding: 20px 0;
        }

            .card-container::-webkit-scrollbar {
                display: none;
            }

        .card {
            flex: 0 0 30%;
            margin: 10px;
            scroll-snap-align: start;
        }

            .card img {
                height: 200px;
                object-fit: cover;
            }

        .nav-button {
            position: absolute;
            top: 50%;
            transform: translateY(-50%);
            background-color: rgba(0, 0, 0, 0.5);
            border: none;
            color: white;
            padding: 10px;
            cursor: pointer;
            z-index: 1;
        }

            .nav-button.left {
                left: 0;
            }

            .nav-button.right {
                right: 0;
            }
    </style>
</head>
<body>
    

    <!-- ======= Header ======= -->
    <header id="header" class="header d-flex align-items-center fixed-top">
        <div class="container-fluid container-xl d-flex align-items-center justify-content-between">
            <img src="~/assets/img/logo.jpeg" class="img-fluid" style="width:6%" />

            <i class="mobile-nav-toggle mobile-nav-show bi bi-list"></i>
            <i class="mobile-nav-toggle mobile-nav-hide d-none bi bi-x"></i>

            <nav id="navbar" class="navbar">
                <ul>
                    <li class="nav-item @(ViewContext.RouteData.Values["controller"].ToString() == "Cource" ? "active" : "")">
                        <a class="nav-link" asp-controller="Home" asp-action="Index">Cource</a>
                    </li>
                    <li class="nav-item @(ViewContext.RouteData.Values["controller"].ToString() == "Coaches" ? "active" : "")">
                        <a class="nav-link" asp-controller="About" asp-action="Index">Coaches</a>
                    </li>
                    <li class="nav-item @(ViewContext.RouteData.Values["controller"].ToString() == "Gallary" ? "active" : "")">
                        <a class="nav-link" asp-controller="Initiatives" asp-action="Index">Gallary</a>
                    </li>
                    
                    <li class="nav-item @(ViewContext.RouteData.Values["controller"].ToString() == "Webinars" ? "active" : "")">
                        <a class="nav-link" asp-controller="Webinars" asp-action="Index">Webinars</a>
                    </li>
                  
                  
                  
                   
                </ul>
            </nav>

        </div>
    </header><!-- End Header -->

    <main id="main">
        <div>
            @RenderBody()
        </div>

    </main>
       


    <!-- ======= Footer ======= -->
    <footer id="footer" class="footer">

        <div class="footer-content">
            <div class="container">
                <div class="row gy-4">
                    <div class="col-lg-5 col-md-12 footer-info">
                        <img src="~/assets/img/logo.jpeg" class="img-fluid" style="width:10%" />
                        <p class="mt-3 " style="flex-wrap: wrap;">ABCD Company provides various Seminar and Workshop on various aspects of life. We want to improve people's skills and empower the every aspects of their Life.</p>
                        <div class="social-links d-flex  mt-3">
                            <a href="#" class="twitter"><i class="bi bi-twitter"></i></a>
                            <a href="#" class="facebook"><i class="bi bi-facebook"></i></a>
                            <a href="#" class="instagram"><i class="bi bi-instagram"></i></a>
                            <a href="#" class="linkedin"><i class="bi bi-linkedin"></i></a>
                        </div>
                    </div>

                    <div class="col-lg-2 col-6 footer-links">
                        <h4>Useful Links</h4>
                        <ul>
                            <li><i class="bi bi-dash"></i> <a href="#">Home</a></li>
                            <li><i class="bi bi-dash"></i> <a href="#">About us</a></li>
                            <li><i class="bi bi-dash"></i> <a href="#">Initiatives</a></li>
                            <li><i class="bi bi-dash"></i> <a href="#">Webinar & Courses</a></li>
                            <li><i class="bi bi-dash"></i> <a href="#">Trainers Profile</a></li>
                            <li><i class="bi bi-dash"></i> <a href="#">ABCD International Talks</a></li>
                            <li><i class="bi bi-dash"></i> <a href="#">Blogs</a></li>
                        </ul>
                    </div>

                    <div class="col-lg-2 col-6 footer-links">
                        <h4>Webinar & Courses</h4>
                        <ul>
                            <li><i class="bi bi-dash"></i> <a href="#">Webinars</a></li>
                            <li><i class="bi bi-dash"></i> <a href="#">Courses</a></li>
                            <li><i class="bi bi-dash"></i> <a href="#">Free Courses</a></li>
                        </ul>
                    </div>

                    <div class="col-lg-3 col-md-12 footer-contact text-center text-md-start">
                        <h4>Contact Us</h4>
                        <p>
                            Juhu, Mumbai<br><br>
                            <strong>Phone:</strong> +919322503465<br>
                            <strong>Email:</strong> abcdinternationalfamily@gmail.com<br>
                        </p>

                    </div>

                </div>
            </div>
        </div>

        <div class="footer-legal" style="background-color:rgba(27, 47, 69, 0.9);">
            <div class="container">
                <div class="copyright text-white">
                    &copy; Copyright <strong><span>ABCD International Family</span></strong>. All Rights Reserved
                </div>

            </div>
        </div>
    </footer><!-- End Footer --><!-- End Footer -->
    <a href="#" class="scroll-top d-flex align-items-center justify-content-center"><i class="bi bi-arrow-up-short"></i></a> 

    <div id="preloader"></div>


    <script src="https://code.jquery.com/jquery-3.5.1.min.js"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.bundle.min.js"></script>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
    <!-- Vendor JS Files -->
    <script src="~/assets/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="~/assets/vendor/aos/aos.js"></script>
    <script src="~/assets/vendor/glightbox/js/glightbox.min.js"></script>
    <script src="~/assets/vendor/swiper/swiper-bundle.min.js"></script>
    <script src="~/assets/vendor/isotope-layout/isotope.pkgd.min.js"></script>
    <script src="~/assets/vendor/php-email-form/validate.js"></script>

    <!-- Template Main JS File -->
    <script src="assets/js/main.js"></script>
</body>
</html>
